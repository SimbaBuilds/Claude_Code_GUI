{
  "name": "claude-code-gui",
  "version": "0.1.0",
  "description": "Browser-based GUI for Claude Code with multi-terminal support and overseer agent",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/server/index.ts",
    "build": "bun build src/client/main.tsx --outdir dist/client --minify",
    "start": "bun run src/server/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.1.62",
    "@anthropic-ai/sdk": "^0.78.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.27",
    "node-pty": "^1.1.0",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.2.1",
    "zustand": "^5.0.11"
  }
}
